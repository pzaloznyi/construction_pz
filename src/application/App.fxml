<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="252.0" xmlns="http://javafx.com/javafx/8.0.45" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AppController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="%file">
          <items>
            <MenuItem mnemonicParsing="false" text="%open" />
            <MenuItem mnemonicParsing="false" text="%save" />
            <MenuItem mnemonicParsing="false" text="%exit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="%selection">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#sort" text="%sort" />
            <MenuItem mnemonicParsing="false" onAction="#printCredits" text="%printcredits" />
            <MenuItem mnemonicParsing="false" onAction="#printDeposits" text="%printdeposits" />
            <MenuItem mnemonicParsing="false" onAction="#printOwners" text="%printowners" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" onAction="#showMessage" text="%help">
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="368.0" prefWidth="281.0" VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="list" layoutX="14.0" layoutY="14.0" prefHeight="335.0" prefWidth="224.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
